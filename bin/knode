#!/usr/bin/env node
require("kaffeine")
var path = require("path")

var x = process.ARGV.length - 1
var file = process.ARGV[x]

if(!file)  {
  console.log("missing source file?")
  return 
}
var abs = path.join(process.cwd(), "./", file).replace(/.k$/, "")

process.chdir(path.dirname(abs))
require(abs)
